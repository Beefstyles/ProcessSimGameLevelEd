<Window x:Class="ProcessSimGameLevelEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessSimLevelEd"
        mc:Ignorable="d"
        Title="ProcessSimLevelEd" Height="350" Width="525">
    <DockPanel>
        <TextBlock DockPanel.Dock ="Top" HorizontalAlignment="Center">ProcessSim Game Level Editor V0.1</TextBlock>
        <StackPanel DockPanel.Dock="Left" HorizontalAlignment="Left">
            <WrapPanel>
                <TextBlock>Methane</TextBlock>
                <TextBox AcceptsReturn="True" TextWrapping="Wrap" Width="55"></TextBox>
            </WrapPanel>
            <WrapPanel>
                <TextBlock>Ethane</TextBlock>
                <TextBox AcceptsReturn="True" TextWrapping="Wrap" Width="55"></TextBox>
            </WrapPanel>
        </StackPanel>
        <Button DockPanel.Dock ="Bottom" HorizontalAlignment="Center" VerticalAlignment="Bottom">Calculate Phase</Button>
    </DockPanel>
    
</Window>
